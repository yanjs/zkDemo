import "hashes/sha256/512bitPadded" as sha256;
from "./Config" import N_MIX;

def contains(\
    u32[N_MIX][8] set,\
    u32[8] elem\
) -> bool {
    // assert set contains elem
    bool mut any_found = false;
    for u32 i in 0..N_MIX {
        any_found = if elem == set[i] {
            true
        } else { any_found };
    }
    return any_found;
}